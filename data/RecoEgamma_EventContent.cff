# Full Event content 
block RecoEgammaFEVT = {
  untracked vstring outputCommands = {
#Electrons
    "keep *_pixelMatchGsfElectrons_*_*",
    "keep *_siStripElectronToTrackAssociator_*_*",
    "keep *_egammaCtfWithMaterialTracks_*_*",
    "keep *_egammaCtfWithMaterialTracksBarrel_*_*",
    "keep *_egammaCtfWithMaterialTracksEndcap_*_*",
    "keep *_siStripElectrons_*_*",
    "keep *_electronPixelSeeds_*_*",

#Photons
    "keep *_photons_*_*",
    "keep *_correctedPhotons_*_*",
    "keep *_convertedPhotons_*_*",
    "keep *_conversionTrackCandidates_*_*",
    "keep *_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep *_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep *_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep *_ckfInOutTracksFromConversionsEndcap_*_*"

  }		
}	

