# Full Event content 
block RecoEgammaFEVT = {
  untracked vstring outputCommands = {
#Electrons
    "keep *_pixelMatchGsfElectrons_*_*",
    "keep *_pixelMatchGsfFit_*_*",
    "keep *_electronPixelSeeds_*_*",

#Photons
    "keep *_photonsWithConversions_*_*",
    "keep *_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep *_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep *_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep *_ckfInOutTracksFromConversionsEndcap_*_*"
  }		
}	

# RECO content
block RecoEgammaRECO = {
  untracked vstring outputCommands = {
#Electrons
    "keep recoGsfElectrons_pixelMatchGsfElectrons_*_*",
    "keep recoGsfTracks_pixelMatchGsfFit_*_*",
    "keep recoGsfTrackExtras_pixelMatchGsfFit_*_*",
    "keep recoTrackExtras_pixelMatchGsfFit_*_*",
    "keep TrackingRecHitsOwned_pixelMatchGsfFit_*_*",
    "keep recoElectronPixelSeeds_electronPixelSeeds_*_*",

#Photons
    "keep recoPhotons_photonsWithConversions_*_*",
    "keep recoConversions_photonsWithConversions_*_*",
    "keep recoTracks_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep recoTracks_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep recoTracks_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep recoTracks_ckfInOutTracksFromConversionsEndcap_*_*",
    "keep recoTrackExtras_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep recoTrackExtras_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep recoTrackExtras_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep recoTrackExtras_ckfInOutTracksFromConversionsEndcap_*_*",
    "keep TrackingRecHitsOwned_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep TrackingRecHitsOwned_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep TrackingRecHitsOwned_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep TrackingRecHitsOwned_ckfInOutTracksFromConversionsEndcap_*_*"
  }	
}	

# AOD content
block RecoEgammaAOD = {
  untracked vstring outputCommands = {
#Electrons
    "keep recoGsfElectrons_pixelMatchGsfElectrons_*_*",
    "keep recoGsfTracks_pixelMatchGsfFit_*_*",

#Photons
    "keep recoPhotons_photonsWithConversions_*_*",
    "keep recoConversions_photonsWithConversions_*_*",
    "keep recoTracks_ckfOutInTracksFromConversionsBarrel_*_*",
    "keep recoTracks_ckfInOutTracksFromConversionsBarrel_*_*",
    "keep recoTracks_ckfOutInTracksFromConversionsEndcap_*_*",
    "keep recoTracks_ckfInOutTracksFromConversionsEndcap_*_*"
  }
}
